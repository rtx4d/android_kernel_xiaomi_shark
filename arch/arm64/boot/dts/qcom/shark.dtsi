#include <dt-bindings/interrupt-controller/irq.h>
#include <dt-bindings/msm/power-on.h>

#include "dsi-panel-synaptics-td4310-video.dtsi"
#include "dsi-panel-synaptics-td4310-cmd.dtsi"

#include "shark-audio-overlay.dtsi"
#include "shark-pinctrl.dtsi"

/ {
	reserved-memory {
		ramoops: ramoops@b0000000 {
			compatible = "ramoops";
			reg = <0 0xb0000000 0 0x00400000>;
			record-size = <0x40000>; /*256x1024*/
			console-size = <0x40000>;
			ftrace-size = <0x40000>;
			pmsg-size = <0x200000>;
			ecc-size = <0x0>;
		};
	};
};

&firmware {
	android {
		compatible = "android,firmware";
		boot_devices = "soc/1d84000.ufshc";
		vbmeta {
			compatible = "android,vbmeta";
			parts = "vbmeta,boot,system,vendor,dtbo";
		};

		fstab {
			compatible = "android,fstab";
			vendor {
				compatible = "android,vendor";
				dev = "/dev/block/platform/soc/1d84000.ufshc/by-name/vendor";
				type = "ext4";
				mnt_flags = "ro,barrier=1,discard";
				fsmgr_flags = "wait,slotselect,avb";
				status = "disabled";
			};
		};
	};
};

&spmi_bus {
	qcom,pm8998@0 {
		qcom,clkdiv@5d00 {
			compatible = "qcom,qpnp-clkdiv";
			reg = <0x5d00 0x100>;
			#clock-cells = <1>;
			qcom,cxo-freq = <19200000>;
			qcom,clkdiv-id = <3>;
			qcom,clkdiv-init-freq = <19200000>;
		};

		qcom,power-on@800 {
			interrupt-names = "kpdpwr", "resin", "kpdpwr-bark",
							  "resin-bark", "kpdpwr-resin-bark";
			interrupts = <0x0 0x8 0x0 IRQ_TYPE_NONE>,
						 <0x0 0x8 0x1 IRQ_TYPE_NONE>,
						 <0x0 0x8 0x3 IRQ_TYPE_NONE>,
						 <0x0 0x8 0x4 IRQ_TYPE_NONE>,
						 <0x0 0x8 0x5 IRQ_TYPE_NONE>;
			
			qcom,pon_1 {
				qcom,support-reset = <1>;
 				qcom,s1-timer = <6720>;
 				qcom,s2-timer = <2000>;
 				qcom,s2-type = <PON_POWER_OFF_HARD_RESET>;
 				qcom,use-bark;
			};

			qcom,pon_3 {
				qcom,s1-timer = <4480>;
 				qcom,s2-timer = <1000>;
 				qcom,s2-type = <PON_POWER_OFF_DVDD_SHUTDOWN>;
			};
		};
	};

	/* shark vbus change to pmi supply */
	qcom,pmi8998@2 {
		qcom,usb-pdphy@1700 {
			vbus-supply = <&smb2_vbus>;
		};
	};

	qcom,pmi8998@3 {
		qcom,leds@d000 {
			qcom,rgb_0 {
				linux,name = "red";
				qcom,default-state = "off";
				linux,default-trigger = "none";
				qcom,start-idx = <0>;
				qcom,idx-len = <48>;
				qcom,turn-off-delay-ms = <500>;
				qcom,duty-pcts =
					[00 00 01 02 03 04 05 06 07 08 09 08 07 06 05 04 03 02 01 00 00 00 00 00
					00 00 00 00 00 00 00 00 00 00 00 00 00 00 00 00 00 00 00 00 00 00 00 00];
				qcom,use-blink;
				qcom,lut-flags = <3>;
				qcom,pause-lo = <0>;
				qcom,pause-hi = <0>;
				qcom,ramp-step-ms = <100>;
				qcom,pwm-us = <1000>;
				qcom,max-current = <12>;
			};

			qcom,rgb_1 {
				linux,name = "green";
				qcom,default-state = "off";
				linux,default-trigger = "none";
				qcom,start-idx = <0>;
				qcom,idx-len = <48>;
				qcom,turn-off-delay-ms = <500>;
				qcom,duty-pcts =
					[00 00 01 02 03 04 05 06 07 08 09 08 07 06 05 04 03 02 01 00 00 00 00 00
					00 00 00 00 00 00 00 00 00 00 00 00 00 00 00 00 00 00 00 00 00 00 00 00];
				qcom,use-blink;
				qcom,lut-flags = <3>;
				qcom,pause-lo = <0>;
				qcom,pause-hi = <0>;
				qcom,ramp-step-ms = <100>;
				qcom,pwm-us = <1000>;
				qcom,max-current = <12>;
			};

			qcom,rgb_2 {
				linux,name = "blue";
				qcom,default-state = "off";
				linux,default-trigger = "none";
				qcom,start-idx = <0>;
				qcom,idx-len = <48>;
				qcom,turn-off-delay-ms = <500>;
				qcom,duty-pcts =
					[00 00 01 02 03 04 05 06 07 08 09 08 07 06 05 04 03 02 01 00 00 00 00 00
					00 00 00 00 00 00 00 00 00 00 00 00 00 00 00 00 00 00 00 00 00 00 00 00];
				qcom,use-blink;
				qcom,lut-flags = <3>;
				qcom,pause-lo = <0>;
				qcom,pause-hi = <0>;
				qcom,ramp-step-ms = <100>;
				qcom,pwm-us = <1000>;
				qcom,max-current = <12>;
			};
		};
	};
};

&mdss_mdp {
	connectors = <&sde_rscc &sde_wb>;
};

&mdss_dsi0_pll {
	qcom,ssc-frequency-hz = <30000>;
	qcom,ssc-ppm = <5000>;
};

&pm8998_l21 {
	regulator-min-microvolt = <3500000>;
	regulator-max-microvolt = <3500000>;
	qcom,init-voltage = <3500000>;
};

&soc {
	dsi_synaptics_td4310_cmd_tianma_display: qcom,dsi-display@29 {
		compatible = "qcom,dsi-display";
		label = "dsi_synap_cmd_tianma_display";
		qcom,display-type = "primary";
		qcom,dsi-ctrl = <&mdss_dsi0>;
		qcom,dsi-phy = <&mdss_dsi_phy0>;
		clocks = <&mdss_dsi0_pll BYTECLK_MUX_0_CLK>,
				<&mdss_dsi0_pll PCLK_MUX_0_CLK>, <&pm8998_div_clk3 0>;
		clock-names = "mux_byte_clk", "mux_pixel_clk", "div_clk3";
		pinctrl-names = "panel_active", "panel_suspend", "default";
		pinctrl-0 = <&sde_dsi_active &sde_te_active>;
		pinctrl-1 = <&sde_dsi_suspend &sde_te_suspend>;
		pinctrl-2 = <&clk_out_default>;
		qcom,dsi-panel = <&dsi_synaptics_td4310_cmd_tianma>;
		vddio-supply = <&pm8998_l14>;
		lab-supply = <&lab_regulator>;
		ibb-supply = <&ibb_regulator>;
	};

	qcom,icnss@18800000 {
		vdd-3.5-fem5G-supply = <&pm8998_l21>;
	};

	qcom,qbt1000 {
		status = "disabled";
	};

	rpmh-regulator-ldoa19 {
		regulator-l19 {
			regulator-max-microvolt = <3310000>;
			qcom,init-voltage = <3100000>;
			regulator-always-on;
		};
	};

	rpmh-regulator-ldoa21 {
		qcom,mode-threshold-currents = <0 800000>;
	};

	fpsensor {
		status = "ok";
		compatible = "goodix,fingerprint";
		vdd_io-supply  = <&pm8998_l19>;
		fp,gpio_rst    = <&tlmm 128 0x00>;
		fp,gpio_irq    = <&tlmm 121 0x00>;
		interrupt-parent = <&tlmm>;
		interrupts = <121 0x0>;
    };

	slide_button {
		compatible = "slide_button";

		pinctrl-names = "default";
		pinctrl-0 = <&slide_button_default>;

		slide-button,gpios = <&tlmm 119 0>;
		debounce-interval = <15>;
		slide-button,wakeup;
	};

	vs,memshare {
		compatible = "vs,memshare";
	};

	restart@10ac000 {
		qcom,force-warm-reboot;
	};
};

&qusb_phy0 {
	qcom,tune-efuse-correction = <0x4>;
};

&pcie0 {
	// hack
	perst-gpio = <&tlmm 999 0>;
