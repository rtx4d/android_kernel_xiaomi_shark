&qupv3_se12_i2c {
	status = "ok";

    tas2559@4c {
        compatible = "ti,tas2559";
        reg = <0x4c>;

        ti,tas2559-reset-gpio = <&tlmm 79 0x00>;
        ti,tas2560-reset-gpio = <&tlmm 6 0x00>;
        ti,tas2559-irq-gpio = <&tlmm 123 0>;
        ti,tas2560-irq-gpio = <&tlmm 122 0>;

        ti,tas2559-addr = <0x4c>;
        ti,tas2560-addr = <0x4e>;
        ti,tas2559-channel = <0>;
        ti,tas2560-channel = <1>;

        ti,ycrc-enable = <1>;
        ti,echo-ref = <0>;
        ti,bit-rate = <16>;
	};
};

&qupv3_se14_i2c {
	status = "ok";

	iris2p@22 {
		compatible = "pxlw,iris2p_i2c";
		reg = <0x22>;
	};
};

&pmi8998_charger {
	qcom,usb-icl-ua = <2800000>;
	qcom,fcc-max-ua = <3500000>;
	qcom,fv-max-uv =  <4400000>;
	qcom,auto-recharge-soc;

	qcom,sw-jeita-enable;
	qcom,battery-data = <&mtp_batterydata>;

	pinctrl-names = "default";
	pinctrl-0 = <&gpio9_dig_out_default>;
	slave-shdn = <&pm8998_gpios 9 GPIO_ACTIVE_HIGH>;
};

&pmi8998_fg {
	qcom,fg-sys-term-current = <(-400)>;
	qcom,fg-chg-term-current = <120>;

	qcom,fg-recharge-soc-thr = <99>;
	qcom,fg-auto-recharge-soc;
	qcom,hold-soc-while-full;
	qcom,linearize-soc;

	qcom,fg-cutoff-voltage   = <3200>;
	qcom,fg-empty-voltage    = <3000>;
	qcom,fg-cc-cv-threshold-mv = <4390>;

	qcom,fg-jeita-thresholds = <0 15 45 55>;
	qcom,fg-aging-jeita-thresholds = <(-20) 15 45 68>;
	qcom,fg-jeita-hyst-temp  = <2>;
	qcom,fg-batt-temp-delta  = <6>;

	qcom,fg-rsense-sel       = <2>;
	qcom,fg-force-load-profile;
};

&wil6210 {
	status = "disabled";
};

&low_soc {
	temperature = <5>;
};

&pm8998_rtc {
	qcom,qpnp-rtc-write = <1>;
	qcom,qpnp-rtc-alarm-pwrup = <1>;
};

&mtp_batterydata {
	qcom,batt-id-range-pct = <50>;
	#include "batterydata-e30-atl-4000mah.dtsi"
	#include "batterydata-e30-sunwoda-4000mah.dtsi"
	/delete-node/ qcom,itech_3000mah;
	/delete-node/ qcom,ascent_3450mah;
	/delete-node/ qcom,Demo_battery_6000mah;
};

&pmi8998_wled {
	status = "okay";
	qcom,fs-curr-ua = <20000>;
	qcom,led-strings-list = [00 01 02 03];
	qcom,dim-mode = "digtal";
	qcom,en-cabc = <1>;
};

&wdog {
	qcom,bark-time = <20000>;
	qcom,pet-time = <15000>;
};

&qupv3_se10_i2c {
	status = "okay";

	smb138x: qcom,smb138x@8 {
		compatible = "qcom,i2c-pmic";
		reg = <0x8>;
		#address-cells = <1>;
		#size-cells = <1>;
		interrupt-parent = <&spmi_bus>;
		interrupts = <0x0 0xd1 0x0 IRQ_TYPE_LEVEL_LOW>;
		interrupt_names = "smb138x";
		interrupt-controller;
		#interrupt-cells = <3>;
		qcom,periph-map = <0x10 0x11 0x12 0x13 0x14 0x16 0x36>;

		smb138x_revid: qcom,revid@100 {
			compatible = "qcom,qpnp-revid";
			reg = <0x100 0x100>;
		};

		smb138x_tadc: qcom,tadc@3600 {
			compatible = "qcom,tadc";
			reg = <0x3600 0x100>;
			#address-cells = <1>;
			#size-cells = <0>;
			#io-channel-cells = <1>;
			interrupt-parent = <&smb138x>;
			interrupts = <0x36 0x0 IRQ_TYPE_EDGE_BOTH>;
			interrupt-names = "eoc";

			batt_temp@0 {
				reg = <0>;
				qcom,rbias = <68100>;
				qcom,rtherm-at-25degc = <68000>;
				qcom,beta-coefficient = <3450>;
			};

			skin_temp@1 {
				reg = <1>;
				qcom,rbias = <33000>;
				qcom,rtherm-at-25degc = <68000>;
				qcom,beta-coefficient = <3450>;
			};

			die_temp@2 {
				reg = <2>;
				qcom,scale = <(-1306)>;
				qcom,offset = <397904>;
			};

			batt_i@3 {
				reg = <3>;
				qcom,channel = <3>;
				qcom,scale = <(-20000000)>;
			};

			batt_v@4 {
				reg = <4>;
				qcom,scale = <5000000>;
			};

			input_i@5 {
				reg = <5>;
				qcom,scale = <14285714>;
			};

			input_v@6 {
				reg = <6>;
				qcom,scale = <25000000>;
			};

			otg_i@7 {
				reg = <7>;
				qcom,scale = <5714286>;
			};
		};

		smb138x_parallel_slave: qcom,smb138x-parallel-slave@1000 {
			compatible = "qcom,smb138x-parallel-slave";
			qcom,pmic-revid = <&smb138x_revid>;
			qcom,parallel-mode = <1>;
			reg = <0x1000 0x700>;
			#address-cells = <1>;
			#size-cells = <1>;
			interrupt-parent = <&smb138x>;

			io-channels =
				<&smb138x_tadc 1>,
				<&smb138x_tadc 2>,
				<&smb138x_tadc 3>,
				<&smb138x_tadc 14>,
				<&smb138x_tadc 15>,
				<&smb138x_tadc 16>,
				<&smb138x_tadc 17>;
			io-channel-names =
				"connector_temp",
				"charger_temp",
				"batt_i",
				"connector_temp_thr1",
				"connector_temp_thr2",
				"connector_temp_thr3",
				"charger_temp_max";

			qcom,chgr@1000 {
				reg = <0x1000 0x100>;
				interrupts = <0x10 0x1 IRQ_TYPE_EDGE_RISING>;
				interrupt-names = "chg-state-change";
			};

			qcom,chgr-misc@1600 {
				reg = <0x1600 0x100>;
				interrupts = <0x16 0x1 IRQ_TYPE_EDGE_RISING>,
					     <0x16 0x6 IRQ_TYPE_EDGE_RISING>;
				interrupt-names = "wdog-bark",
						  "temperature-change";
			};

			smb138x_vbus: qcom,smb138x-vbus {
				status = "disabled";
				regulator-name = "smb138x-vbus";
			};
		};
	};
};
